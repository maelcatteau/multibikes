#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/multibikes_website_website_sale_renting_patch.js:0
#, javascript-format
msgid "You cannot pick up your rental on that day of the week."
msgstr "Sie können Ihre Anmietung an diesem Wochentag nicht abholen."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/multibikes_website_website_sale_renting_patch.js:0
#, javascript-format
msgid "You cannot return your rental on that day of the week."
msgstr "Sie können Ihre Anmietung an diesem Wochentag nicht zurückgeben."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/multibikes_website_website_sale_renting_patch.js:0
#, javascript-format
msgid "The return date should be after the pickup date."
msgstr "Das Rückgabedatum muss nach dem Abholdatum liegen."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/multibikes_website_website_sale_renting_patch.js:0
#, javascript-format
msgid "The pickup date cannot be in the past."
msgstr "Das Abholdatum darf nicht in der Vergangenheit liegen."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/multibikes_website_website_sale_renting_patch.js:0
#, javascript-format
msgid "The rental duration is too short. The minimum rental period is %s between %s and %s."
msgstr "Die Mietdauer ist zu kurz. Die Mindestmietdauer beträgt %s zwischen %s und %s."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/multibikes_website_website_sale_renting_patch.js:0
#, javascript-format
msgid "The rental duration is too short. The minimum rental period is %s."
msgstr "Die Mietdauer ist zu kurz. Die Mindestmietdauer beträgt %s."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/multibikes_website_website_sale_renting_patch.js:0
#, javascript-format
msgid "Please select a rental period."
msgstr "Bitte wählen Sie eine Mietdauer aus."