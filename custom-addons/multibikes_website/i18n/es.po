#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/website_sale.js:0
#, javascript-format
msgid "You cannot pick up your rental on that day of the week."
msgstr "No puede recoger su alquiler en ese día de la semana."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/website_sale.js:0
#, javascript-format
msgid "You cannot return your rental on that day of the week."
msgstr "No puede devolver su alquiler en ese día de la semana."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/website_sale.js:0
#, javascript-format
msgid "The return date should be after the pickup date."
msgstr "La fecha de devolución debe ser posterior a la fecha de recogida."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/website_sale.js:0
#, javascript-format
msgid "The pickup date cannot be in the past."
msgstr "La fecha de recogida no puede estar en el pasado."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/website_sale.js:0
#, javascript-format
msgid "The rental duration is too short. The minimum rental period is %s between %s and %s."
msgstr "La duración del alquiler es demasiado corta. El período mínimo de alquiler es de %s entre %s y %s."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/website_sale.js:0
#, javascript-format
msgid "The rental duration is too short. The minimum rental period is %s."
msgstr "La duración del alquiler es demasiado corta. El período mínimo de alquiler es de %s."

#. module: multibikes_website
#: code:addons/multibikes_website/static/src/js/website_sale.js:0
#, javascript-format
msgid "Please select a rental period."
msgstr "Por favor, seleccione un período de alquiler."
